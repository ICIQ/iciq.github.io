	Journal = {Journal of Scientific Computing},
	Keywords = {Mathematics and Statistics},
	Month = dec,
	Number = {2},
	Pages = {216--238--238},
	Publisher = {Springer Netherlands},
	Title = {{Improvement of Convergence to Steady State Solutions of Euler Equations with the WENO Schemes}},
