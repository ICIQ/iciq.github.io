	Keywords = {Runge-Kutta,SSP,conservation law,initial value problem,method of,strong stability preservation},
	Local-Url = {/Users/ketch/Documents/Mendeley Desktop/Ferracina, Spijker/2005 - Stepsize restrictions for total-variation-boundedness in general Runge-Kutta procedures.pdf},
	Mendeley-Tags = {Runge-Kutta,SSP,strong stability preservation},
